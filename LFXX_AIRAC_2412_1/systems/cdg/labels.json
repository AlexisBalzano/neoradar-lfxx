{
  "$schema": "https://raw.githubusercontent.com/neoradar-project/schemas/refs/heads/main/systems/labels.schema.json",
  "ground": {
    "default": [
      [
        {
          "itemName": "callsign",
          "prefix": ["=assumedPrefix"],
          "fontWeight": "bold",
          "fontSize": 12
        },
        {
          "itemName": "squawk",
          "prefix": "@"
        }
      ]
    ],
    "departure": [
      [
        {
          "itemName": "callsign",
          "prefix": ["=assumedPrefix"],
          "fontWeight": "bold",
          "fontSize": 12,
          "color": "=incorrectSquawkRedColor"
        },
        {
          "itemName": "text",
          "value": "=penWhenScratchpadNotEmpty",
          "fontSize": 12,
          "fontWeight": "bold"
        },
        {
          "itemName": "acShortType",
          "placeholder": "ATYP"
        }
      ],
      [
        {
          "itemName": "text",
          "placeholder": "RWY",
          "value": ["=suggestedDepRunwayIfNoAssigned", "&depRunway"]
        },
        {
          "itemName": "text",
          "placeholder": "SID",
          "value": ["=suggestedSidIfNoAssigned", "&sid"]
        }
      ]
    ],
    "arrival": [
      [
        {
          "itemName": "callsign",
          "prefix": ["=assumedPrefix"],
          "fontWeight": "bold",
          "fontSize": 12
        },
        {
          "itemName": "acShortType",
          "placeholder": "ATYP"
        }
      ],
      [
        {
          "itemName": "scratchpad"
        }
      ]
    ],
    "detailed": [
      [
        {
          "itemName": "callsign",
          "fontWeight": "bold",
          "leftClick": "callsignMenu",
          "prefix": ["=assumedPrefix"],
          "fontSize": 12

        },
        {
          "itemName": "squawk",
          "prefix": "@"
        }
      ]
    ],
    "departureDetailed": [
      [
        {
          "itemName": "callsign",
          "fontWeight": "bold",
          "fontSize": 12,
          "leftClick": "callsignMenu",
          "prefix": ["=assumedPrefix"],
          "color": "=incorrectSquawkRedColor"
        },
        {
          "itemName": "acShortType",
          "placeholder": "ATYP",
          "leftClick": "fpDialog"
        }
      ],
      [
        {
          "itemName": "text",
          "placeholder": "RWY",
          "value": ["=suggestedDepRunwayIfNoAssigned", "&depRunway"],
          "leftClick": "depRunwayMenu"
        },
        {
          "itemName": "text",
          "placeholder": "SID",
          "value": ["=suggestedSidIfNoAssigned", "&sid"],
          "leftClick": "sidMenu"
        }
      ],
      [
        {
          "itemName": "groundStatus",
          "leftClick": "groundStatusMenu",
          "mapping": {
            "0": "NOST",
            "1": "STUP",
            "2": "PUSH",
            "3": "TAXI",
            "4": "DEPA"
          }
        },
        {
          "itemName": "scratchpad",
          "leftClick": "triggerScratchpad",
          "placeholder": "..."
        }
      ]
    ],
    "arrivalDetailed": [
      [
        {
          "itemName": "callsign",
          "fontWeight": "bold",
          "fontSize": 12,
          "leftClick": "callsignMenu",
          "prefix": ["=assumedPrefix"]
        },
        {
          "itemName": "acShortType",
          "placeholder": "ATYP",
          "leftClick": "fpDialog"
        }
      ],
      [
        {
          "itemName": "scratchpad",
          "placeholder": "...",
          "leftClick": "triggerScratchpad"
        }
      ]
    ],
    "styleVariants": [
      {
        "applyWhen": "$isOutboundTransfer",
        "backgroundColor": [34, 117, 78],
        "textColor": [255, 255, 255],
